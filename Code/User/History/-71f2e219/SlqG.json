            "invalid_directory": "invalid"
        },
        "proxy": {
            "type": "http",
            "directory": "proxies",
            "proxy_format": "ip:port"
        }   
    },
    "account": {
        "2take1": {
            "URL": "https://2take1.menu/login",
        }
    }
}